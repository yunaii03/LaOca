def disparar_coordenadas():
   tt = tabl_pc()
   tab = pedir_coordenadas()
   pz = input("Le tienes miedo a la guerra??")
   total = 0
   totalll_s = 0
   tabll_vacio = tabl_vacio()
   confirmador_s = 0
   confirmador_j = 0

   while pz!="si" and total != 17 or totalll_s !=17:

       while pz!="si" and total != 17 or totalll_s !=17:

           pz = input("Quieres huir??")
           print("Comandante es tu turno, coordenadas exactas para disparar: ")
           px = int(input("Dime la fila: "))
           py = int(input("Dime la columna: "))
           kkkk = input("Pulsa . para disparar")
           while pz!="si" and total != 17 or totalll_s !=17:

               if tt[px][py] == '○':
                   tt[px][py] = 'X'
                   tabll_vacio[px][py] = 'X'
                   confirmador_j = confirmador_j + 1
                   total = total + 1

                   print("Le has dado a un barco")
                   for ii in tabll_vacio:
                       for ll in tabll_vacio[ii]:
                           print(ll, end=" ")
                       print()
                   print()
               elif tt[px][py] == '-':
                   tt[px][py] = "*"
                   tabll_vacio[px][py] = '*'
                   print("!!!!!No le has dado a ningun barco!!!!!")
                   confirmador_s = confirmador_s + 1

                   for ii in tabll_vacio:
                       for ll in tabll_vacio[ii]:
                           print(ll, end=" ")
                       print()
                   print()
               elif tt[px][py] == 'X':

                   print("!!!!Ya le has dado ha ese barco!!!!!")


                   confirmador_s = confirmador_s + 1
                   for ii in tabll_vacio:
                       for ll in tabll_vacio[ii]:
                           print(ll, end=" ")
                       print()
                   print()



               kk = input("Pulsa . para cambiar de turno")
               if kk == ".":

                   break
           if  kkkk==".":

               break











       while pz!="si" and total != 17 or totalll_s !=17:

           ll = random.randint(0, 9)
           vv = random.randint(0, 9)

           print("!!!!Turno de Siri...!!!!")
           kkkk = input("Pulsa . para disparar")
           print("Siri ha disparado en la casilla", ll, vv)

           while totalll_s !=17:
               if tab[ll][vv] == '○':
                   tab[ll][vv] = 'X'
                   print("!!!!Siri ha dado a un barco!!!!", ll, vv)
                   confirmador_s = confirmador_s +1
                   totalll_s = totalll_s + 1

                   for ii in tab:
                       for pp in tab[ii]:
                           print(pp, end=" ")
                       print()
                   print()

               elif tab[ll][vv] == '-':
                   tab[ll][vv] = '*'
                   print("!!!!!Siri no le ha dado ha ningun barco!!!!!")

                   for ii in tab:
                       for pp in tab[ii]:
                           print(pp, end=" ")
                       print()
                   print()

               elif tab[ll][vv] == '*':
                   print("Siri ya habia disparado en esta posicion")
                   for ii in tab:
                       for pp in tab[ii]:
                           print(pp, end=" ")
                       print()
                   print()

               elif tab[ll][vv] == 'X':
                   print("!!!!Siri ya le ha dado ha ese barco !!!!!")

                   for ii in tab:
                       for pp in tab[ii]:
                           print(pp, end=" ")
                       print()
                   print()

               pz = input("Quieres terminar el combate??")

               kk = input("Pulsa . para cambiar de turno")
               if kk == ".":

                   break
           if kkkk == ".":

               break
           if total == 17:
               print("Has ganado")

       if  total==17:
        print("!!!!!Has ganado!!!!")
        print("!!!Bien hecho comandante!!!")
        break
       if totalll_s==17:
        print("!!!!!Ha ganado Siri!!!!")
        print("La proxima guerra ira mejor...")
        break


